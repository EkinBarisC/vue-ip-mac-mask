  },
  "devDependencies": {
    "vite": "^5.0.0",
    "vue": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EkinBarisC/vue-ip-mac-mask.git"
  },
  "homepage": "https://github.com/EkinBarisC/vue-ip-mac-mask#readme",
