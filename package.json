  "description": "Vue 3 directive for IPv4, IPv6, and MAC address input masking with validation",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./src/index.d.ts"
